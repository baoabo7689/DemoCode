@model GamesAdmin.Site.Features.GameTicketDetail.ViewModels.GameTicketDetailViewModel

    <div class="col-md-12 col-lg-6 card">
        <form class="card-body form-add form" asp-action="Index" asp-controller="GameTicketDetail" method="post" target="_blank">

            <div class="form-group">
                <label asp-for="MemberId" class="control-label"></label>
                <input type="text" asp-for="MemberId" class="form-control" />
            </div>

            <div class="form-group">
                <div>
                    <label asp-for="GameRoundId" class="control-label"></label>
                    <input type="text" asp-for="GameRoundId" class="form-control" />
                </div>
            </div>

            <div class="form-group">
                <div>
                    <label asp-for="GameType" class="control-label"></label>
                    <select asp-items="@Model.GameTypeItem" asp-for="GameType" class="form-control"></select>
                </div>
            </div>
            <div class="form-group">
                <div>
                    <label asp-for="Language" class="control-label"></label>
                    <select asp-items="@Model.LanguageItem" asp-for="Language" class="form-control"></select>
                </div>
            </div>
            <div class="form-group">
                <div>
                    <label asp-for="Currency" class="control-label"></label>
                    <input type="text" asp-for="Currency" class="form-control" />
                </div>
            </div>
            <div class="form-group">
                <input type="submit" value="Get Ticket Detail" class="btn-submit" />
                <input type="submit" value="Retrieve End Game Info" class="btn-submit" formaction="/RetrieveEndGameInfo/RetrieveEndGame"/>
            </div>            
        </form>        
    </div>


